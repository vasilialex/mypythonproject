


lst = [1, 2, 3, 4, 5]
lst2 = []

for i in lst:
    lst2.append(i * 2)
print(lst2)

comp = [num * 2 for num in lst]
print(comp)

comp2 = [numb *2 for numb in range(1, 6) ]
print(comp2)

comp3 = []
for i in range(1, 6):
    comp3.append(i * 2)
print(comp3)

my_list = [num * 2 for num in range(10, 1, -2)]
print(my_list)

words = ["guitar", "balalaika", "hui"]

wors_dot = [word+"." for word in words if len(word) > 3]
print(wors_dot)